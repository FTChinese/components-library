[
    {
        "moduleName": "ftc-icons",
        "tagName": "3.0.0",
        "repoHomeUrl": "https://github.com/FTChinese/ftc-icons",
        "keywords": [
            "FTC",
            "icons"
        ],
        "hasCss": true,
        "hasJs": false,
        "dependencies": {
            "o-colors": "^3.5.2",
            "o-assets": "^3.0.2"
