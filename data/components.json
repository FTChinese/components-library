                "description": "use svg symbol as icon image"
            },
            {
                "name": "svg-symbol-dark",
                "template": "demos/src/share.html",
                "svgSymbol": true,
                "theme": "symbol-dark",
                "data": "demos/src/data.json",
                "description": "use svg symbol as icon image"
            },
            {
                "name": "svg-symbol-tint",
                "template": "demos/src/share.html",
                "svgSymbol": true,
                "theme": "symbol-tint",
                "data": "demos/src/data.json",
